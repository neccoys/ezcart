{
    "name": "neccoys/ezcart",
    "description": "A easy cart for Laravel",
    "keywords": [
        "laravel",
        "cart",
        "shopping",
        "shopping cart"
    ],
    "homepage": "https://github.com/neccoys/ezcart",
    "license": "MIT",
    "authors": [
        {
            "name": "neccoys",
            "email": "neccoys@example.com",
            "homepage": "https://example.com"
        }
    ],
    "require": {
        "php": "~7.3 || ~7.4 || ~8.0",
        "ext-intl": "*",
        "illuminate/support": "~5.5.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0",
        "illuminate/session": "~5.5.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0",
        "illuminate/events": "~5.5.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.25"
    },
    "autoload": {
        "psr-4": {
            "Neccoys\\EzCart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neccoys\\EzCart\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Neccoys\\EzCart\\EzCartServiceProvider"
            ],
            "aliases": {
                "EzCart": "Neccoys\\EzCart\\Facades\\EzCart"
            }
        }
    },
    "minimum-stability": "stable"
}
